{
  "name": "nota-ai-commit-lainbo",
  "displayName": "Nota AI Commit",
  "description": "Use Azure/OpenAI/Gemini API to review Git changes, generate conventional commit messages that meet the conventions, simplify the commit process, and keep the commit conventions consistent.",
  "version": "0.1.14",
  "engines": {
    "node": ">=16",
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Azure",
    "OpenAI",
    "ChatGPT",
    "Gemini",
    "GitEmoji",
    "Git Commit",
    "Conventional Commits",
    "Commitizen",
    "Commit Message",
    "Commit Message Generator",
    "AI Commit",
    "Auto Commit"
  ],
  "publisher": "lainbo",
  "repository": "https://github.com/lainbo/ai-commit",
  "icon": "images/logo.png",
  "activationEvents": [
    "onCommand:extension.ai-commit",
    "onCommand:ai-commit.showAvailableModels"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.ai-commit",
        "icon": "images/logo1.svg",
        "title": "%aiCommit.command.aiCommit.title%"
      },
      {
        "command": "ai-commit.showAvailableModels",
        "title": "%aiCommit.command.showAvailableModels.title%"
      }
    ],
    "configuration": [
      {
        "title": "%aiCommit.configuration.general.title%",
        "properties": {
          "ai-commit.AI_PROVIDER": {
            "type": "string",
            "default": "openai",
            "description": "%aiCommit.config.aiProvider.description%",
            "enum": [
              "openai",
              "gemini"
            ]
          },
          "ai-commit.DIFF_SOURCE": {
            "type": "string",
            "default": "auto",
            "enum": [
              "auto",
              "staged",
              "unstaged",
              "staged+unstaged"
            ],
            "description": "%aiCommit.config.diffSource.description%"
          },
          "ai-commit.AI_COMMIT_LANGUAGE": {
            "default": "English",
            "description": "%aiCommit.config.commitLanguage.description%",
            "enum": [
              "Simplified Chinese",
              "Traditional Chinese",
              "Japanese",
              "Korean",
              "Czech",
              "German",
              "French",
              "Italian",
              "Dutch",
              "Portuguese",
              "Vietnamese",
              "English",
              "Spanish",
              "Swedish",
              "Russian",
              "Bahasa",
              "Polish",
              "Turkish",
              "Thai"
            ],
            "enumDescriptions": [
              "简体中文",
              "繁體中文",
              "にほんご",
              "한국어",
              "česky",
              "Deutsch",
              "française",
              "italiano",
              "Nederlands",
              "português",
              "tiếng Việt",
              "english",
              "español",
              "Svenska",
              "русский",
              "bahasa",
              "Polski",
              "Turkish",
              "ไทย"
            ],
            "type": "string"
          },
          "ai-commit.AI_COMMIT_SYSTEM_PROMPT": {
            "type": "string",
            "default": "",
            "description": "%aiCommit.config.systemPrompt.description%"
          },
          "ai-commit.SCM_INPUT_BEHAVIOR": {
            "type": "string",
            "default": "ignore",
            "enum": [
              "ignore",
              "context"
            ],
            "enumDescriptions": [
              "%aiCommit.config.scmInputBehavior.enum.ignore%",
              "%aiCommit.config.scmInputBehavior.enum.context%"
            ],
            "description": "%aiCommit.config.scmInputBehavior.description%"
          }
        }
      },
      {
        "title": "%aiCommit.configuration.git.title%",
        "properties": {
          "ai-commit.REFERENCE_GIT_LOG": {
            "type": "boolean",
            "default": true,
            "description": "%aiCommit.config.referenceGitLog.description%"
          },
          "ai-commit.GIT_LOG_COUNT": {
            "type": "number",
            "default": 20,
            "minimum": 1,
            "maximum": 50,
            "description": "%aiCommit.config.gitLogCount.description%"
          },
          "ai-commit.GIT_LOG_AUTHOR_SCOPE": {
            "type": "string",
            "default": "all",
            "enum": [
              "all",
              "self"
            ],
            "enumDescriptions": [
              "%aiCommit.config.gitLogAuthorScope.enum.all%",
              "%aiCommit.config.gitLogAuthorScope.enum.self%"
            ],
            "description": "%aiCommit.config.gitLogAuthorScope.description%"
          }
        }
      },
      {
        "title": "%aiCommit.configuration.openai.title%",
        "properties": {
          "ai-commit.OPENAI_API_KEY": {
            "default": "",
            "description": "%aiCommit.config.openaiApiKey.description%",
            "type": "string"
          },
          "ai-commit.OPENAI_BASE_URL": {
            "default": "",
            "description": "%aiCommit.config.openaiBaseUrl.description%",
            "type": "string"
          },
          "ai-commit.AZURE_API_VERSION": {
            "default": "",
            "description": "%aiCommit.config.azureApiVersion.description%",
            "type": "string"
          },
          "ai-commit.OPENAI_MODEL": {
            "default": "gpt-4o",
            "description": "%aiCommit.config.openaiModel.description%",
            "type": "string"
          },
          "ai-commit.OPENAI_TEMPERATURE": {
            "type": "number",
            "default": 0.7,
            "minimum": 0,
            "maximum": 2,
            "description": "%aiCommit.config.openaiTemperature.description%"
          }
        }
      },
      {
        "title": "%aiCommit.configuration.gemini.title%",
        "properties": {
          "ai-commit.GEMINI_API_KEY": {
            "type": "string",
            "default": "",
            "description": "%aiCommit.config.geminiApiKey.description%"
          },
          "ai-commit.GEMINI_BASE_URL": {
            "type": "string",
            "default": "",
            "description": "%aiCommit.config.geminiBaseUrl.description%"
          },
          "ai-commit.GEMINI_MODEL": {
            "type": "string",
            "default": "gemini-2.0-flash-001",
            "description": "%aiCommit.config.geminiModel.description%"
          },
          "ai-commit.GEMINI_TEMPERATURE": {
            "type": "number",
            "default": 0.7,
            "minimum": 0,
            "maximum": 2,
            "description": "%aiCommit.config.geminiTemperature.description%"
          }
        }
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "extension.ai-commit",
          "group": "navigation"
        }
      ]
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "build": "webpack --mode production --devtool hidden-source-map",
    "compile": "webpack",
    "compile-tests": "tsc -p . --outDir out",
    "format": "prettier --write \"src/**/*.ts\" \"*.json\" \".vscode/*.json\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"*.json\" \".vscode/*.json\"",
    "lint": "eslint src --ext ts",
    "package": "vsce package --no-dependencies",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "publish": "vsce publish --no-dependencies",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run build",
    "watch": "webpack --watch",
    "watch-tests": "tsc -p . -w --outDir out"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "prettier": "^3.7.4",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "fs-extra": "^11.0.4",
    "openai": "^4.14.2",
    "simple-git": "^3.17.0"
  },
  "resolutions": {
    "@types/node": "16.x"
  },
  "license": "MIT"
}
